<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <!-- Usuario -->
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.aplicationpaw.views.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/bibliotecaFragment"
        android:name="com.example.aplicationpaw.views.ui.biblioteca.BibliotecaFragment"
        android:label="Biblioteca"
        tools:layout="@layout/fragment_biblioteca" />
    <fragment
        android:id="@+id/perfilFragment"
        android:name="com.example.aplicationpaw.views.ui.perfil.PerfilFragment"
        android:label="Perfil"
        tools:layout="@layout/fragment_perfil" />
    <fragment
        android:id="@+id/mapFragment"
        android:name="com.example.aplicationpaw.views.ui.mapa.MapFragment"
        android:label="fragment_map"
        tools:layout="@layout/fragment_map" />
    <dialog
        android:id="@+id/requestDialogFragment"
        android:name="com.example.aplicationpaw.views.ui.dialogo.RequestDialogFragment"
        android:label="fragment_request_dialog"
        tools:layout="@layout/fragment_request_dialog" />

    <!-- Paseador -->
    <fragment
        android:id="@+id/homePaseadorFragment"
        android:name="com.example.aplicationpaw.views.ui.home_paseador.HomePaseadorFragment"
        android:label="fragment_home_paseador"
        tools:layout="@layout/fragment_home_paseador" />
    <fragment
        android:id="@+id/perfilPaseadorFragment"
        android:name="com.example.aplicationpaw.views.ui.perfil_paseador.PerfilPaseadorFragment"
        android:label="fragment_perfil_paseador"
        tools:layout="@layout/fragment_perfil_paseador" />


</navigation>